{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "object-curly-newline": ["error", { "multiline": true }],
    "consistent-return": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
